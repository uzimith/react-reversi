{
  "name": "react-reversi",
  "version": "0.0.1",
  "description": "reversi with web",
  "repository": "http://github.com/uzimith/react-reversi",
  "main": "app.coffee",
  "scripts": {
    "start": "watchify -o bundle.js -v -d .",
    "test": "jest"
  },
  "author": "",
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true
        }
      ],
      "coffeeify",
      "babelify",
      "react-jade"
    ]
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testFileExtensions": [
      "js",
      "coffee"
    ],
    "scriptPreprocessor": "<rootDir>/preprocessor.js"
  },
  "dependencies": {
    "babelify": "^5.0.3",
    "bluebird": "^2.9.12",
    "coffee-script": "^1.9.1",
    "express": "^4.12.0",
    "flummox": "^2.6.2",
    "lodash": "^3.2.0",
    "react": "^0.13.0-beta.2",
    "socket.io": "^1.3.4"
  },
  "devDependencies": {
    "browserify": "^8.1.0",
    "coffeeify": "^1.0.0",
    "gulp": "^3.8.10",
    "gulp-plumber": "^0.6.6",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.2",
    "gulp-watch": "^4.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "jade": "^1.9.2",
    "react-jade": "^2.3.0",
    "reactify": "^0.17.1",
    "watchify": "^2.2.1"
  }
}
